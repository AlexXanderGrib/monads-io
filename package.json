{
  "name": "monads-io",
  "version": "5.0.0",
  "description": "ðŸš€ Efficient Monads for JS: Maybe (Option) and Either (Result), Identity",
  "scripts": {
    "test": "jest",
    "build:dist": "rimraf dist && rollup -c",
    "build:docs": "typedoc",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*"
  },
  "packageManager": "npm@11.3.0",
  "main": "./index.js",
  "module": "./index.mjs",
  "type": "commonjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./either": {
      "require": "./either.js",
      "import": "./either.mjs",
      "types": "./either.d.ts"
    },
    "./maybe": {
      "require": "./maybe.js",
      "import": "./maybe.mjs",
      "types": "./maybe.d.ts"
    },
    "./identity": {
      "require": "./identity.js",
      "import": "./identity.mjs",
      "types": "./identity.d.ts"
    },
    "./types": {
      "types": "./types.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/monads-io.git"
  },
  "keywords": [
    "monads",
    "either",
    "maybe",
    "option",
    "result",
    "front-end",
    "backend"
  ],
  "files": [
    "dist/",
    "src/",
    "coverage/",
    "examples/",
    "index.js",
    "index.mjs",
    "index.d.ts",
    "either.js",
    "either.mjs",
    "either.d.ts",
    "identity.js",
    "identity.mjs",
    "identity.d.ts",
    "maybe.js",
    "maybe.mjs",
    "maybe.d.ts",
    "types.d.ts",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md"
  ],
  "author": "Alexander G <me@alexxgrib.me> (https://alexxgrib.me/)",
  "maintainers": [
    "Alexander G <me@alexxgrib.me> (https://alexxgrib.me/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/monads-io/issues"
  },
  "homepage": "https://github.com/AlexXanderGrib/monads-io#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^59.0.0",
    "glob": "^11.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-prettier": "^4.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.6.3",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=14"
  }
}
